package starbucks ;

/** My Cards Screen */
public class MyCards extends Screen
{
	String cardBalance = "$0.00";
	
    public MyCards()
    {
       	 /**
         * Get Display Contents
         * @return Display Contents
         */
    }

    public String GetCardBalance() { 
        
        return cardBalance; 
    }
    
    public String display() { 
               
        return this.GetCardBalance(); 
    }
   
}
