package starbucks;

/** My Card Options Screen */
public class MyCardsOptions extends Screen
{
   
    public MyCardsOptions()
    {
       
    }

    /**
     * Display MyCardsOptions available options
     * @return cardOptions for Display
     */
    public String display()
    {
    	StringBuilder cardOptions = new StringBuilder();
    	
    	cardOptions.append("Reload\n");
    	cardOptions.append("Refresh\n");
    	cardOptions.append("More Options\n");
    	cardOptions.append("Cancel");
    
    	String out="";
    	out += "Refresh\n";
    	out += "Reload\n";
    	out += "Auto Reload\n";
    	out += "Transactions";
    	
    	return cardOptions.toString();
    			
    }
}
